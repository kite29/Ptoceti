<div  class="row">
	<div class="col-md-12 col-sm-12 col-xs-12">
		<p><span>{{watchtext.watches}}</span><span name="count" class="badge pull-right"/></p>
	</div>
</div>
<div class="row">
	<div class="col-md-12 col-sm-12 hidden-xs">
		<p>
		<div class="btn-group">
			<button id="removeButton" type="button" class="btn btn-default {{#unless onXsMedia }} btn-lg {{/unless}}" disabled="disabled">
			  <span class="glyphicon glyphicon-minus"></span> {{watchtext.remove}}
			</button>
			<button id="createButton" type="button" class="btn btn-default {{#unless onXsMedia }} btn-lg {{/unless}}">
			  <span class="glyphicon glyphicon-plus"></span> {{watchtext.create}}
			</button>
			<button id="reloadButton" type="button" class="btn btn-default {{#unless onXsMedia }} btn-lg {{/unless}}">
			  <span class="glyphicon glyphicon-refresh"></span> {{watchtext.reload}}
			</button>
		</div>
		</p>
	</div>
</div>


<div id="watchList" class="row">
	
</div>